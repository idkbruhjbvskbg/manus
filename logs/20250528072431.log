const canvas = document.getElementById('physicsCanvas');
const ctx = canvas.getContext('2d');

canvas.width = 600;
canvas.height = 400;

const ball = {
    x: canvas.width / 2,
    y: 50,
    radius: 10,
    color: 'blue',
    dy: 0, // velocity in y direction
    gravity: 0.5,
    bounce: 0.7 // energy retained after bounce
};

function drawBall() {
    ctx.beginPath();
    ctx.arc(ball.x, ball.y, ball.radius, 0, Math.PI * 2);
    ctx.fillStyle = ball.color;
    ctx.fill();
    ctx.closePath();
}

function update() {
    // Apply gravity
    ball.dy += ball.gravity;
    // Update position
    ball.y += ball.dy;

    // Check for collision with bottom boundary
    if (ball.y + ball.radius > canvas.height) {
        ball.y = canvas.height - ball.radius; // Reposition ball
        ball.dy = -ball.dy * ball.bounce; // Reverse direction and apply bounce
    }

    // Clear canvas
    ctx.clearRect(0, 0, canvas.width, canvas.height);

    // Draw ball
    drawBall();

    // Request next frame
    requestAnimationFrame(update);
}

// Start the animation loop
update();
```

